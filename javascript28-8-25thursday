let vowelsArray = ["A", "E", "I", "O", "U"];

for (let i = 0; i < students.length; i++) {
  let name = students[i];
  if (vowelsArray.includes(name[0].toUpperCase())) {
    vowel.push(name);
  } else {
    consonants.push(name);
  }
}
